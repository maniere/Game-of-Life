//**************************  INIT CELLS  ******************************
void initCells() {
  for (int cell_x = 0; cell_x < COLS; cell_x++) {
    for (int cell_y = 0; cell_y < ROWS; cell_y++) {
      cellAges[cell_x][cell_y] = 0;
      if (random(1.0) < rand_seed) {  // optionally randomly seed some cells
        cells[cell_x][cell_y] = 1.0;
      }
      else {
        cells[cell_x][cell_y] = 0.0;
      }
    }
  }
}

//**************************  APPLY RULES  *****************************
void applyRules() {
  for (int cell_x = 0; cell_x < COLS; cell_x++) {
    for (int cell_y = active_y; cell_y < ROWS; cell_y++) {
      applyRulesToCell(cell_x, cell_y);
    }
  }
}

//**********************  APPLY RULES TO CELL  *************************
  // 1) Stay alive if it haa two or three neighbors, otherwise die
  // 2) Come to life if it has exactly three neighbors
void applyRulesToCell(int x, int y) {
  int sum = int(sumNeighbors(x,y));  // get the sum of its neighbors
    // Apply rule 1:
  if (cells[x][y] == 1.0) {  // currently alive?
    if (sum == 2 || sum == 3) {
      nextCells[x][y] = 1.0; // survive
    }
    else {
      nextCells[x][y] = 0.0; // die
    }
  }
    // Apply rule 2:
  if (cells[x][y] == 0.0) {  // currently dead?
    if (sum == 3) {
      nextCells[x][y] = 1.0; // come to life
    }
  }
    // Add randomness (turned off by setting rand_life to 0)
  if (random(1.0) < rand_life) {
    nextCells[x][y] = 1.0 - nextCells[x][y];
  }
    // Update the number of generations this cell has been alive
  if (cells[x][y] == 1.0 && nextCells[x][y] == 1.0) {
    cellAges[x][y]++;
  }
  else {
    cellAges[x][y] = 0;
  }
}

//*************************  SUM NEIGHBORS  ****************************
  // Sums the states of all neighboring cells. In the typical game, this
  // is the same as counting the number of living cells that are neighbors
  // (because alive is state 1.0, and dead is state 0.0)
float sumNeighbors(int x, int y) {
  float sum = 0.0;
  for (int dir = 0; dir < 8; dir++) {
    sum += checkNeighbor(x, y, dir);
  }
  return sum;
}

//************************  CHECK NEIGHBORS  ***************************
float checkNeighbor(int x, int y, int direction) {
  int xOffset = 0;
  int yOffset = 0;
  int xNei, yNei;
  if (direction == UP || direction == UP_RIGHT || direction == UP_LEFT) {
    yOffset = -1;
  }
  else if (direction == DOWN || direction == DOWN_RIGHT || direction == DOWN_LEFT) {
    yOffset = 1;
  }
  if (direction == RIGHT || direction == UP_RIGHT || direction == DOWN_RIGHT) {
    xOffset = 1;
  }
  else if (direction == LEFT || direction == DOWN_LEFT || direction == UP_LEFT) {
    xOffset = -1;
  }
    // Do wrap-around in X
  xNei = x + xOffset;
  if (xNei < 0) {
    xNei = COLS - 1;
  }
  else if (xNei >= COLS) {
    xNei = 0;
  }
    // Do wrap-around in Y
  yNei = y + yOffset;
  if (yNei < (0 + active_y)) {
    yNei = ROWS - 1;
  }
  else if (yNei >= ROWS) {
    yNei = 0 + active_y;
  }
  return cells[xNei][yNei];
}

//*****************************  DO IT  ********************************
void doIt() {
  if (!mouse_active) { // check is needed to allow for mouse interactions to persist when paused
    applyRules();
  }
  background(BACKGROUND_COLOR);
  applyNewValues();
  drawCells();
  if (DRAW_GRID) {
    drawGrid();
  }
}

//***********************  APPLY NEW VALUES  ***************************
void applyNewValues() {
  for (int cell_x = 0; cell_x < COLS; cell_x++) {
    for (int cell_y = 0; cell_y < ROWS; cell_y++) {
      cells[cell_x][cell_y] = nextCells[cell_x][cell_y];
    }
  }
}

//**************************  DRAW CELLS  ******************************
void drawCells() {
  for (int cell_x = 0; cell_x < COLS; cell_x++) {
    for (int cell_y = 0; cell_y < ROWS; cell_y++) {
      drawCell(cell_x, cell_y);
    }
  }
}

//*************************  DRAW ONE CELL  ****************************
void drawCell(int x, int y) {
  color generationColor;
  if (cells[x][y] == 1.0) {
     // Shift the cell color from NEW_CELL_COLOR to OLD_CELL_COLOR as it's alive for successive generations
    int shiftFactor = min(COLOR_SHIFT_GENS, cellAges[x][y]);
    generationColor = color(
      red(NEW_CELL_COLOR) - shiftFactor * (red(NEW_CELL_COLOR) - red(OLD_CELL_COLOR)) / COLOR_SHIFT_GENS,
      green(NEW_CELL_COLOR) - shiftFactor * (green(NEW_CELL_COLOR) - green(OLD_CELL_COLOR)) / COLOR_SHIFT_GENS,
      blue(NEW_CELL_COLOR) - shiftFactor * (blue(NEW_CELL_COLOR) - blue(OLD_CELL_COLOR)) / COLOR_SHIFT_GENS
    );
    noStroke();
    fill(generationColor);
    rect(cellWidth * x + MARGIN_LEFT, cellHeight * y + MARGIN_TOP, cellWidth, cellHeight);
  }
}

//***************************  DRAW GRID  ******************************
void drawGrid() {
  noFill();
  stroke(GRID_LINE_COLOR);
  for (int cell_x = 0; cell_x < COLS+1; cell_x++) {
    line(cellWidth * cell_x + MARGIN_LEFT, MARGIN_TOP, cellWidth * cell_x + MARGIN_LEFT, GRID_HEIGHT + MARGIN_TOP);
  }
  for (int cell_y = 0; cell_y < (ROWS + MARGIN_TOP / cellHeight + MARGIN_BOTTOM / cellHeight); cell_y++) {
    line(0, cellHeight * cell_y, width, cellHeight * cell_y);
  }
}

