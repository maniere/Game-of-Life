    // Set the first active row to the one just below the space we've reserved for text
  active_y = TEXT_AREA_HEIGHT;
    // Sum the widths of the text, as identified by the successive offsets per each font
  for (int i = 0; i < text.length; i++) {
    textWidth += text[i][text[i].length-1];
  }
    // Calculate in which column to begin the text to horizontally center it
  if ( (COLS - textWidth) % 2 == 0 ) { // the text can be centered without a fraction
    char_x = (COLS - textWidth) / 2;
  }
  else { // Shift the text left one column rather than using a fraction or rounding up
    char_x = (COLS - textWidth - 1) / 2;
  }
    // Calculate in which row to begin the text to vertically center it in the inactive space
  char_y = (int)(active_y / 2 + FONT_HEIGHT / 2);
  for (int i = 0; i < text.length; i++) { // loop through the text characters
    charOffset     =  nextCharOffset;
    nextCharOffset =  text[i][text[i].length-1];
    char_x         += charOffset;
    charIdx        =  0;
    fontWidth      =  (text[i].length - 1) / FONT_HEIGHT;
    for (int yOffset = 0; yOffset < FONT_HEIGHT; yOffset++) { // loop through the rows, bottom to top
      cell_y = char_y - yOffset;
      for (int xOffset = 0; xOffset < fontWidth; xOffset++) { // loop through the cells, left to right
        cell_x = char_x + xOffset;
        if ( nextCells[cell_x][cell_y] == 0.0 ) { // handle possible kerning / applying on a pre-seeded grid 
          nextCells[cell_x][cell_y] = (float)text[i][charIdx];
        }
        charIdx++;
      }
    }
  }
}

//**********************  DO PATTERN OR LINES  *************************
void doPatternOrLines(boolean best) {
  float random1 = random(1);
  if (random1 < 0.15) { // 15% chance to draw lines
    doLines();
  }
  else { // 85% chance to draw a pattern
    doRandomPattern(best);
  }
}

//***********************  DO RANDOM PATTERN  **************************
void doRandomPattern(boolean best) {
  int pattern[];
  if (best) {
    pattern = bestpatterns[(int)(random(bestpatterns.length))];
  }
  else {
    pattern = patterns[(int)(random(patterns.length))];
  }
  doPattern(pattern);
}
  
//***************************  DO PATTERN  *****************************
void doPattern(int[] pattern) { // center the pattern in the active area and draw it
  int pattWidth     = pattern[pattern.length - 2];
  int pattHeight    = pattern[pattern.length - 1];
  int patt_x        = int((COLS - pattWidth) / 2);
  int patt_y        = ROWS - (int)((ROWS - active_y - pattHeight) / 2);
  int cell_x;
  int cell_y;
  int pattIdx = 0;
  for (int yOffset = 0; yOffset < pattHeight; yOffset++) {
    cell_y = patt_y - yOffset;
    for (int xOffset = 0; xOffset < pattWidth; xOffset++) {
      cell_x = patt_x + xOffset;
      nextCells[cell_x][cell_y] = (float)pattern[pattIdx];
      pattIdx++;
    }
  }
}

//****************************  DO LINES  ******************************
void doLines() {
  int num_v;                 // the number of vertical lines
  int num_h;                 // the number of horizontal lines
  boolean center_h = false;  // center the horizontal lines
  boolean center_v = false;  // center the vertical lines
  float random1;             // whether to center the lines
  int loc;                   // the random location parameter for a line
  int offset;                // the offset from center for centered lines
    // draw 0-7 vertical lines
  num_v = (int)(random(7));
    // draw 0 - 5 horizontal lines
  num_h = (int)(random(5));
    // make sure to show something
  if (num_v == 0 && num_h == 0) {
    doLines();
  }
  random1 = random(1);
  if (random1 < .55) { // 55% chance lines will be both vertically and horizontally centered
    center_h = true;
    center_v = true;
  }
  else if (random1 >= .55 && random1 < .70) { // 15% chance horizontal lines will be vertically centered
    center_h = true;
  }
  else if (random1 >= .70 && random1 < .85) { // 15% chance vertical lines will be horizontally centered
    center_v = true;
  }
  else { // 15% chance no lines will be centered
  }
    // do the vertical lines
  for (int i = 0; i < num_v; i++) {
      // if we are centering the lines, there will be an odd number of them, and this is the first one
    if (center_v && (num_v % 2 != 0) && i == 0) {
        // draw the line in the horizontal center
      loc = (int)(COLS / 2);
      drawVerticalLine(loc);
    }
      // otherwise, if we are centering the lines and there will be at least two more
    else if (center_v && (num_v - i >= 2)) { // draw them equidistant from the center
      offset = (int)(random(COLS / 2));
      loc = (int)(COLS / 2) + offset;
      drawVerticalLine(loc);
      loc = (int)(COLS / 2) - offset;
      drawVerticalLine(loc);
      i++;
    }
      // otherwise, either we are not centering the lines or there remains only one left to draw
    else {  // draw the line in a random location
      loc = (int)(random(COLS));
      drawVerticalLine(loc);
    }
  }
    // do the horizontal lines
  for (int i = 0; i < num_h; i++) {
      // if we are centering the lines, there will be an odd number of them, and this is the first one
    if (center_h && (num_h % 2 != 0) && i == 0) {
      loc = (int)((ROWS - active_y) / 2) + active_y;
        // draw the line in the vertical center of the active area
      drawHorizontalLine(loc);
    }
      // otherwise, if we are centering the lines and there will be at least two more
    else if (center_h && (num_h - 1 >= 2)) { // draw them equidistant from the center
      offset = (int)(random((int)((ROWS - active_y) / 2)));
      loc = (int)(((ROWS - active_y) / 2) + active_y) + offset;
      drawHorizontalLine(loc);
      loc = (int)(((ROWS - active_y) / 2) + active_y) - offset;
      drawHorizontalLine(loc);
      i++;
    }
      // otherwise, either we are not centering the lines or there remains only one left to draw
    else {
      loc = (int)(random(ROWS - active_y) + active_y);
      drawHorizontalLine(loc);
    }
  }
}

//**********************  DRAW HORIZONTAL LINE  ************************
void drawHorizontalLine(int yOrig) {
  int cell_x = 0;
  int cell_y = yOrig;
  for (int xOffset = 0; xOffset < COLS; xOffset++) {
    nextCells[cell_x++][cell_y] = 1.0;
  }
}

//***********************  DRAW VERTICAL LINE  *************************
void drawVerticalLine(int xOrig) {
  int cell_x = xOrig;
  int cell_y = active_y;
  for (int yOffset = active_y; yOffset < ROWS; yOffset++) {
    nextCells[cell_x][cell_y++] = 1.0;
  }
}

//**********************  RESET CURRENT STAGE  *************************
public void resetCurrentStage() {
  killCells(0,0,COLS,ROWS);
  initCells();
  if (active_y != 0) {
    doText(title);
  };
}

//***************************  KILL CELLS  *****************************
void killCells(int x0, int y0, int x1, int y2) {
  for (int cell_x = x0; cell_x < x1; cell_x++) {
    for (int cell_y = y0; cell_y < y2; cell_y++) {
      cellAges[cell_x][cell_y] = 0;
      cells[cell_x][cell_y] = 0.0;
      nextCells[cell_x][cell_y] = 0.0;
    }
  }
}

//****************************  INTERACT  ******************************
//**********  MOUSE  ***********
void interact() {
  mouse_active = true;
  getCell();
  if (mcx != mcx_last || mcy != mcy_last || newClick) {  // if the mouse is interacting with a new cell
    flipCell();
    newClick = false;
  }
}

void getCell() {
  int mx,my;
  mcx_last = mcx;
  mcy_last = mcy;
  mx  = constrain(mouseX - MARGIN_LEFT, 0, GRID_WIDTH - 1);
  //mcx = max((int)(mx / cellWidth) - 1, 0);
  mcx = max((int)((mx + (3 * cellWidth / 4)) / cellWidth) - 1, 0);
  my  = constrain(mouseY - MARGIN_TOP, 0, GRID_HEIGHT - 1);
  //mcy = max((int)(my / cellHeight) - 1, 0);
  mcy = max((int)((my + (3 * cellHeight / 4)) / cellHeight) - 1, 0);
}

void flipCell() {
  if (cells[mcx][mcy] == 1.0) {
    nextCells[mcx][mcy] = 0.0;
  }
  else {
    nextCells[mcx][mcy] = 1.0;
  }
  doIt();
}

void mouseReleased() { 
  mouse_active = false;
  newClick = true;
}

